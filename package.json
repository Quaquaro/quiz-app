{
  "name": "quiz-app",
  "version": "1.0.0",
  "description": "- HTML - CSS - JS",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run htmlhint && npm run prettier:check && npm run eslint:check",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
    "test:jest":"NODE_OPTIONS=--experimental-vm-modules npx jest",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write .",
    "htmlhint": "npx htmlhint '**/*.html'",
    "eslint:check": "npx eslint .",
    "eslint:write": "npx eslint --fix .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quaquaro/quiz-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Quaquaro/quiz-app/issues"
  },
  "homepage": "https://github.com/Quaquaro/quiz-app#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/html": "^6.4.13",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "htmlhint": "^1.1.1",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "prettier": "2.5.1"
  },
  "dependencies": {
    "npm": "^8.3.2",
    "stable": "^0.1.8"
  }
}
